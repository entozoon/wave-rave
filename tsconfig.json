{
  "compilerOptions": {
    "outDir": "./build/",
    "sourceMap": true,
    "strictNullChecks": false,
    "module": "commonjs",
    "target": "es5",
    "downlevelIteration": true,
    "allowJs": true,
    "noImplicitAny": false
  },
  "exclude": ["./node_modules/*", "./build", "./static"],
  "typeRoots": ["./node_modules/phaser/types"],
  "types": ["Phaser"]
}
