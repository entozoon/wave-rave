{
  "name": "wave-rave",
  "version": "1.0.0",
  "description": "Rad game; no doubt, no doubt.",
  "main": "src/game.ts",
  "keywords": [
    "phaser",
    "typescript"
  ],
  "scripts": {
    "prestart": "open-cli http://localhost:8080",
    "start": "cross-env NODE_ENV=development nollup -c --hot --content-base public",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c",
    "serve": "npm run build && serve dist"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.0",
    "cross-env": "^7.0.2",
    "nollup": "^0.10.7",
    "phaser": "^3.22.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.29.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-static-files": "^0.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "serve": "^11.3.0",
    "typescript": "3.8.3",
    "open-cli": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entozoon/wave-rave.git"
  },
  "author": {
    "name": "Michael Cook"
  },
