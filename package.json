{
  "name": "@phaserjs/phaser-rollup-typescript-template",
  "version": "1.0.0",
  "description": "Phaser 4 Rollup + TypeScript Template",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve public"
  },
  "devDependencies": {
    "@phaserjs/phaser": "^0.1.9",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.20.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^6.1.0",
    "serve": "^11.3.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.6"
  }
}
