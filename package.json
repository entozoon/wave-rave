  "bugs": {
    "url": "https://github.com/entozoon/wave-rave/issues"
  },
  "homepage": "https://github.com/entozoon/wave-rave#readme",
  "dependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-uglify": "^6.0.4"
  }
}
